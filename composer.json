            "ChristianJombo\\NigerianPhone\\Test": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "ChristianJombo\\NigerianPhone\\NigerianPhoneServiceProvider"
